{
  "name": "zynqa/filament-freeagent",
  "description": "FreeAgent integration for FilamentPHP",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Zynqa",
      "email": "info@zynqa.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "filament/filament": "^3.2",
    "spatie/laravel-settings": "^3.0",
    "spatie/laravel-package-tools": "^1.16",
    "illuminate/support": "^11.0",
    "guzzlehttp/guzzle": "^7.8"
  },
  "require-dev": {
    "orchestra/testbench": "^9.0",
    "pestphp/pest": "^2.0",
    "pestphp/pest-plugin-laravel": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Zynqa\\FilamentFreeAgent\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Zynqa\\FilamentFreeAgent\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Zynqa\\FilamentFreeAgent\\FilamentFreeAgentServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
